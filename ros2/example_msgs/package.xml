<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
    <name>example_msgs</name>
    <version>0.0.1</version>
    <description>Messages used for showing rcl handle use</description>

    <author>Griswald Brooks</author>
    <maintainer email="griswald.brooks@picknik.ai">Griswald Brooks</maintainer>

    <license>Prorietary</license>

    <url type="bugtracker">https://github.com/PickNikRobotics/ros_testing_templates/issues</url>
    <url type="repository">https://github.com/PickNikRobotics/ros_testing_templates</url>

    <buildtool_depend>ament_cmake</buildtool_depend>
    <buildtool_depend>rosidl_default_generators</buildtool_depend>

    <depend>action_msgs</depend>

    <exec_depend>rosidl_default_runtime</exec_depend>
    <member_of_group>rosidl_interface_packages</member_of_group>
    <export>
        <build_type>ament_cmake</build_type>
    </export>
